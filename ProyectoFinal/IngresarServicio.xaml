<Window x:Class="ProyectoFinal.IngresarServicio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="IngresarServicio" Height="300" Width="300">
    <Grid>
        <Button x:Name="btnGuardar" Content="Guardar" HorizontalAlignment="Left" Margin="111,218,0,0" VerticalAlignment="Top" Width="75" Click="btnGuardar_Click"/>
        <Label Content="Nombre:" HorizontalAlignment="Left" Margin="27,60,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtNombre" HorizontalAlignment="Left" Height="23" Margin="111,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Descripcion:" HorizontalAlignment="Left" Margin="27,126,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="txbDescripcion" HorizontalAlignment="Left" Margin="111,112,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="54" Width="120"/>

    </Grid>
</Window>
